      "source": [
        "import astropy.units as u"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "H = 70 * (u.km / u.s) / u.Mpc # Hubble constant\n",
        "T_0 = 1 / H\n",
        "T_0.to(u.year)"
      ],

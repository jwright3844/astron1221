    {
      "cell_type": "code",
      "source": [
        "# find time for galaxies moving at 70 km/s to move 1 Mpc apart\n",
        "\n",
        "t = 1 * u.Mpc / (70 * u.km / u.s)\n",
        "t.to(u.year)"
      ],
